import java.util.ArrayList;
public class Main
{
	public static void main(String[] args) {
		int input1=3;
		int i=0;
		int [] input2={-1,9,0,5,-4,-6,-3};
		ArrayList<Integer> a= new ArrayList();
		int max=input2[0];
		for(i=1;i<input2.length;i++){
		    if(input2[i]>=0){
		        a.add(input2[i]);
		    }
		   
		}
		System.out.println(a);
		for(i=0;i<a.size();i++){
		     max=max+(i+2)*a.get(i);
		}
		System.out.println(max);
		
	}
}
